<table width="100%">
<% 5.times do %>
  <tr>
  <% 5.times do %>
    <td>
      <% if local_assigns[:m] %>
        <% e = m.shift %>
        <% c = 'cs' if e == 'cs' %>
        <% c = 'blank' if e == '--' %>
        <% c = 'legal' if e == 'kk' %>
        <div class="square small <%=c%>"></span></div>
      <% end %>
    </td>
  <% end %>
  </tr>
<% end %>
</table>
